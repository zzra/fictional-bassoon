<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whatshouldiwear">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_appicon_01"
        android:supportsRtl="true"
        android:theme="@style/Theme.WhatShouldIWear">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditHead"></activity>
        <activity android:name=".EditTop"></activity>
        <activity android:name=".EditBottom"></activity>
    </application>

</manifest>